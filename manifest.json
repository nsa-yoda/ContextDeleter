{
  "name": "__MSG_extName__",
  "short_name": "ContextDeleter",
  "version": "3.0.0",
  "manifest_version": 3,
  "author": "Juan L. Sanchez",
  "description": "__MSG_extDesc__",
  "permissions": [
    "contextMenus",
    "storage"
  ],
  "host_permissions": [
    "http://*/*",
    "https://*/*",
    "https://fonts.googleapis.com/*"
  ],
  "content_security_policy": {
  },
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "jquery.min.js",
        "content.js"
      ]
    }
  ],
  "minimum_chrome_version": "88.0.0.0",
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "64": "icons/64.png",
    "128": "icons/128.png",
    "256": "icons/256.png",
    "512": "icons/512.png"
  },
  "default_locale": "en"
}
